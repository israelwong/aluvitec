---
import Layout from "../components/Layout.astro";
const response = await fetch("https://jsonplaceholder.typicode.com/posts");
const data = await response.json();

---

<Layout title="Posts">
  <h1>Mis posts</h1>

  <ul>
    {
      data.map((user:any) => (
        <div>
          <h1>{user.id}</h1>
          <p>{user.title}</p>
          <p>{user.body}</p>
        </div>
      ))
    }
  </ul>
</Layout>
